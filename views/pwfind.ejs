  <div class="site-wrap">
    <!--네비-->
    
      <%- include('./partial/navbar(logout).ejs') %>

    <div class="site-section">
      <div class="container">
        <div class="row mb-5">
          <div class="col-md-12">
            <div class="border p-4 rounded" role="alert">
              비밀번호를 알고 계신가요? <a href="/login">로그인</a> 하러가기
            </div>
          </div>
        </div>
        <form  action="/pwfind" method="post">
